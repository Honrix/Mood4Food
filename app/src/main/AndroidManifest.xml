<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jrk.mood4food">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="com.jrk.mood4food.App"
        >
        <activity android:name=".NavBarActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".waterbalance.view.WaterBalanceActivity" />
        <activity android:name=".SettingsActivity" />
        <activity android:name=".NutrientActivity" />
        <activity android:name=".recipes.view.RecipeActivity" />

        <activity android:name=".TestActivity" />

    </application>

</manifest>